<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MauiApp2.MainPage">
    <StackLayout>
        <Button Text="Walmart" Clicked="OnStoreButtonClicked" CommandParameter="contentwalmart.png" />
        <Button Text="Nike" Clicked="OnStoreButtonClicked" CommandParameter="contentnike.png" />
    

        <Image x:Name="storeImage" WidthRequest="200" HeightRequest="200" />
    </StackLayout>
</ContentPage>
